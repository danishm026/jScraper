<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context" 
    xmlns:aop="http://www.springframework.org/schema/aop"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd 
    http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-3.0.xsd
    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd ">
    
    <context:annotation-config />
    <aop:aspectj-autoproxy />
    
    <bean id="homePageParser" class="com.arc.jScraper.parsers.HomePageParser">
    	<property name="url" value="http://www.celebwallpaper.org/" />
    </bean>    
    
    <bean id="categoryPageParser" class="com.arc.jScraper.parsers.CategoryPageParser" />
    
    <bean id="modelPageParser" class="com.arc.jScraper.parsers.ModelPageParser" />
    
    <bean id="imagePageParser" class="com.arc.jScraper.parsers.ImagePageParser" />
    
    <bean id="modelPageParserWrapper" class="com.arc.jScraper.parsers.ModelPageParserWrapper" />
    
    <bean id="loggingAspect" class="com.arc.jScraper.aspects.LoggingAspect" />
    
    <bean id="scraper" class="com.arc.jScraper.parsers.Scraper" />
    
    <bean id="model" class="com.arc.jScraperDao.dto.Model" scope="prototype"/>
</beans>